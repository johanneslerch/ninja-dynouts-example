rule dynoutRule
   command = cmd /C echo test>$out&&echo foobar>output.dynamic&&(echo ninja_dyndep_version = 1 & echo build $out ^| output.dynamic : dyndep) > dynout

build output.static | dynout: dynoutRule input
   dyndep = dynout

rule conflict
   command = cmd /C echo test>$out

build output.dynamic: conflict

default output.static
